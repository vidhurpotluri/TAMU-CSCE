/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=w);
    Not(in=b,out=e);
    Nor(a=w,b=e,out=out);
}
